function buscarMob() {
    const termo = document.getElementById("searchInput").value.toLowerCase();
    let resultados = [];

    for (let mob of dados) {
        if (mob["Nome do Mob"].toLowerCase().includes(termo)) {
            resultados.push(`<strong>${mob["Nome do Mob"]}</strong><br>Número: ${mob["Número"]}<br>Arquivo: ${mob["Arquivo"]}<br><br>`);
        }
    }

    document.getElementById("resultado").innerHTML = resultados.length > 0
        ? resultados.join("")
        : "Nenhum mob encontrado.";
}
